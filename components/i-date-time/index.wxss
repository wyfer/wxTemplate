.i-as{position:fixed;width:100%;box-sizing:border-box;left:0;right:0;bottom:0;background:#fff;transform:translate3d(0,100%,0);transform-origin:center;transition:all .2s ease-in-out;z-index:900;visibility:hidden}.i-as-show{transform:translate3d(0,0,0);visibility:visible}.i-as-mask{position:fixed;top:0;left:0;right:0;bottom:0;background:rgba(0,0,0,.4);z-index:900;transition:all .2s ease-in-out;opacity:0;visibility:hidden}.i-as-mask-show{opacity:1;visibility:visible}.picker-cell{width:100%;height:135px;margin:40rpx 0}.picker-cell-column{width:50%;text-align:center}.d-text{line-height:45px;font-weight:600}.division{font-weight:600;color:#06a8fe;line-height:135px}.show-date{width:100%;text-align:left;font-size:30rpx;height:100rpx;font-weight:600;background:#fff;display:flex;justify-content:space-between;align-items:center;border-bottom:solid 1rpx #f5f5f5}.time{padding-left:20rpx}.btn{padding-right:20rpx}.chose-btn{background:#06a8fe;color:#fff;padding:8rpx 25rpx;border-radius:5rpx}.chose-err{background:#999;color:#333}